{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./src",
    "paths": {
      
      "styles/*": ["./styles/*"],
      "components/*": ["./components/*"],
      "constants/*": ["./constants/*"],
      "pages/*": ["./pages/*"],
      "api/*": ["./api/*"],
      "assets/*": ["./assets/*"],
      "stores/*": ["./stores/*"],
      "interfaces/*": ["./interfaces/*"],
      "utils/*": ["./utils/*"],
      "@/*": ["./*"],

    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
